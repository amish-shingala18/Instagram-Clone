<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    >
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <LinearLayout
            android:id="@+id/lnrSearch"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginHorizontal="5dp"
            android:orientation="horizontal"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">
            <LinearLayout
                android:layout_width="124dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">
                <ImageView
                    android:id="@+id/imgSearch1"
                    android:layout_width="match_parent"
                    android:layout_height="124dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/amish"
                    android:layout_marginEnd="1dp"/>
                <ImageView
                    android:id="@+id/imgSearch2"
                    android:layout_width="match_parent"
                    android:layout_height="124dp"
                    android:layout_marginTop="2dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/amish4"
                    android:layout_marginEnd="1dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="124dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">
                <ImageView
                    android:id="@+id/imgSearch3"
                    android:layout_width="match_parent"
                    android:layout_height="124dp"
                    android:layout_marginHorizontal="1dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/amish4" />
                <ImageView
                    android:id="@+id/imgSearch4"
                    android:layout_width="match_parent"
                    android:layout_height="124dp"
                    android:layout_marginTop="2dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/amish"
                    android:layout_marginHorizontal="1dp"/>
            </LinearLayout>
            <ImageView
                android:id="@+id/imgSearch5"
                android:layout_width="124dp"
                android:layout_height="250dp"
                android:layout_weight="1"
                android:scaleType="centerCrop"
                android:src="@drawable/amish3"
                android:layout_marginStart="1dp"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/lnrSearch"
            android:layout_marginVertical="2dp"
            android:layout_marginHorizontal="5dp"
            android:orientation="horizontal">
            <ImageView
                android:id="@+id/imgSearch6"
                android:layout_width="124dp"
                android:layout_height="250dp"
                android:layout_weight="1"
                android:scaleType="centerCrop"
                android:src="@drawable/amish3"
                android:layout_marginEnd="1dp"/>
            <LinearLayout
                android:layout_width="124dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">
                <ImageView
                    android:id="@+id/imgSearch7"
                    android:layout_width="match_parent"
                    android:layout_height="124dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/amish4"
                    android:layout_marginHorizontal="1dp"/>
                <ImageView
                    android:id="@+id/imgSearch8"
                    android:layout_width="match_parent"
                    android:layout_height="124dp"
                    android:scaleType="centerCrop"
                    android:layout_marginTop="2dp"
                    android:src="@drawable/amish"
                    android:layout_marginHorizontal="1dp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="124dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">
                <ImageView
                    android:id="@+id/imgSearch9"
                    android:layout_width="match_parent"
                    android:layout_height="124dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/amish"
                    android:layout_marginStart="1dp"/>
                <ImageView
                    android:id="@+id/imgSearch10"
                    android:layout_width="match_parent"
                    android:layout_height="124dp"
                    android:layout_marginTop="2dp"
                    android:scaleType="centerCrop"
                    android:src="@drawable/amish4"
                    android:layout_marginStart="1dp"/>
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>